<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel - Skill Sharing Platform</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <div class="container">
        <div class="nav-container">
            <a href="/user-portal">User Portal</a>
            <a href="/tutor-dashboard">Tutor Dashboard</a>
            <a href="/admin-panel">Admin Panel</a>
            <a href="/landing-page">Landing Page</a>
            <a href="/blog">Blog</a>
        </div>
        
        <h1>Admin Panel</h1>
        <h2>User Management</h2>
        <!-- Code to manage users -->
        <div id="user-management">
            <!-- Placeholder for user management interface -->
            <p>Manage user profiles here.</p>
            <!-- Example form to view users -->
            <form action="/manage-users" method="GET">
                <button type="submit">View Users</button>
            </form>
        </div>

        <h2>Tutor Management</h2>
        <!-- Code to manage tutors -->
        <div id="tutor-management">
            <!-- Placeholder for tutor management interface -->
            <p>Manage tutor profiles here.</p>
            <!-- Example form to view tutors -->
            <form action="/manage-tutors" method="GET">
                <button type="submit">View Tutors</button>
            </form>
        </div>

        
        <h2>Skill Categories</h2>
        <!-- Code to manage skill categories -->
        <div id="skill-categories">
            <!-- Placeholder for skill category management interface -->
            <p>Manage skill categories here.</p>
            <!-- Example form to view skill categories -->
            <form action="/manage-skill-categories" method="GET">
                <button type="submit">View Skill Categories</button>
            </form>
        </div>

        <h2>Bookings and Payments</h2>
        <!-- Code to manage bookings and payments -->
        <div id="bookings-payments">
            <!-- Placeholder for bookings and payments management interface -->
            <p>Manage bookings and payments here.</p>
            <!-- Example form to view bookings -->
            <form action="/manage-bookings" method="GET">
                <button type="submit">View Bookings</button>
            </form>
            <!-- Example form to view payments -->
            <form action="/manage-payments" method="GET">
                <button type="submit">View Payments</button>
            </form>
        </div>

        <h2>Analytics and Reporting</h2>
<!-- Code to view analytics and reports -->
<div id="analytics-reporting">
    <!-- Placeholder for analytics and reporting interface -->
    <p>View analytics and reports here.</p>
    <!-- Example form to view analytics -->
    <form action="/view-analytics" method="GET">
        <button type="submit">View Analytics</button>
    </form>
    <!-- Example form to view reports -->
    <form action="/view-reports" method="GET">
        <button type="submit">View Reports</button>
    </form>
</div>

        <a href="/logout" class="logout-link">Logout</a>
    </div>
</body>
</html>
